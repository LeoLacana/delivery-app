{
  "name": "delivery-app-back-end",
  "version": "0.1.0",
  "description": "Back-end do projeto App de Delivery",
  "main": "./src/api/server.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "engineStrict": true,
  "private": true,
  "scripts": {
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "start": "node .",
    "dev": "nodemon .",
    "test": "mocha src/tests/**/*$NAME*.test.js --exit",
    "test:coverage": "nyc --all npm run test",
    "test:coverage:json": "nyc --all --reporter=json-summary npm run test",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "dependencies": {
    "@hapi/boom": "9.1.2",
    "@types/express": "4.17.11",
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rescue": "1.1.26",
    "joi": "^17.5.0",
    "jsonwebtoken": "8.5.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "multer": "1.4.2",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4",
    "socket.io": "3.0.4"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "eslint-config-trybe-backend": "1.0.3",
    "mocha": "8.4.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "sequelize-cli": "6.2.0",
    "sinon": "10.0.0"
  }
}
